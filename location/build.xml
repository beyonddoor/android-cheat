<?xml version="1.0" encoding="UTF-8"?>
<project name="AndroidDebugLocation">
    <property environment="env" />
    <condition property="sdk.dir" value="${env.ANDROID_HOME}">
        <isset property="env.ANDROID_HOME" />
    </condition>
    <property name="target" value="${env.ANDROID_TARGET}" />
    <property name="android.library.reference.1" value="${env.ANDROID_HOME}/extras/google/google_play_services/libproject/google-play-services_lib"/>
    <import file="${sdk.dir}/tools/ant/build.xml" />
</project>
